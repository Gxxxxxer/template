{
    "presets": [
        ["env", {
            "modules": false
        }],
        "stage-0",
        "stage-1",
        "stage-2",
        "stage-3",
        "react"
    ],
    "plugins": [
        "react-loadable/babel",
        "transform-decorators-legacy",
        "react-hot-loader/babel",
        ["react-css-modules", {
            "generateScopedName": "[path]_[name]_[local]_[hash:5]",
            "filetypes": {
                ".styl": {
                    "syntax": "sugarss"
                }
            }
        }]
    ]
}
